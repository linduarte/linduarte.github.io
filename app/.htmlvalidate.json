{
    "extends": [
        "html-validate:recommended"
    ],
    "rules": {
        "doctype-first": "off",
        "no-dup-attr": "error",
        "void-style": [
            "error",
            {
                "style": "omit"
            }
        ],
        "no-inline-style": "error",
        "attribute-empty-style": "off",
        "no-raw-characters": "error"
    }
}
